args = "import os"

[[workflows.workflow.tasks]]
task = "shell.exec"
args = ""

[[workflows.workflow.tasks]]
task = "shell.exec"
args = "class UnrestrictedHandler(http.server.SimpleHTTPRequestHandler):"

[[workflows.workflow.tasks]]
task = "shell.exec"
args = "    def end_headers(self):"

[[workflows.workflow.tasks]]
task = "shell.exec"
args = "        self.send_header('Cache-Control', 'no-cache, no-store, must-revalidate')"

