
# AI Studio Pro+ v10.0.0 - Production Environment Variables
# Copy this file to .env and add your real API keys

# OpenAI Configuration
OPENAI_API_KEY=sk-your-openai-api-key-here
OPENAI_ORG_ID=org-your-organization-id

# Additional AI Service Keys (Optional)
STABILITY_API_KEY=sk-your-stability-ai-key
ELEVENLABS_API_KEY=your-elevenlabs-key
RUNWAY_API_KEY=your-runway-ml-key

# Production Configuration
NODE_ENV=production
PORT=5000
HOST=0.0.0.0

# Feature Flags
ENABLE_OPENAI=true
ENABLE_IMAGE_GENERATION=true
ENABLE_VOICE_SYNTHESIS=true
ENABLE_VIDEO_GENERATION=true
CORS_DISABLED=true
SECURITY_RESTRICTIONS=false
